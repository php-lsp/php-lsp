{
    "name": "php-lsp/hydrator-jms",
    "description": "JMS Hydrator Implementation",
    "type": "library",
    "license": "MIT",
    "keywords": ["hydrator", "mapping", "serialization", "jms", "implementation"],
    "support": {
        "source": "https://github.com/php-lsp/hydrator-jms",
        "issues": "https://github.com/php-lsp/hydrator-jms/issues"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru",
            "homepage": "https://nesk.me"
        }
    ],
    "require": {
        "php": "^8.1",
        "jms/serializer": "^3.0",
        "php-lsp/hydrator-contracts": "^1.0",
        "psr/cache": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Lsp\\Hydrator\\JMS\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Lsp\\Hydrator\\JMS\\Tests\\": "tests"
        }
    },
    "provide": {
        "php-lsp/hydrator-contracts-implementation": "^1.0"
    },
    "scripts": {
        "test": "@test:unit",
        "test:unit": "phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "bin-compat": "full",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
